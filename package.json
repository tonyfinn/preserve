{
    "name": "preserve",
    "version": "0.2.4",
    "description": "Dense Media Player for Jellyfin",
    "main": "index.js",
    "dependencies": {
        "@types/hls.js": "^0.13.0",
        "bowser": "^2.10.0",
        "hls.js": "^0.14.9",
        "jellyfin-apiclient": "^1.4.1",
        "uuid": "^8.3.0",
        "vue": "^3.0.0-rc.10"
    },
    "sideEffects": [
        "*.scss",
        "*.css",
        "*.vue"
    ],
    "devDependencies": {
        "@types/bowser": "^1.1.1",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "@vue/compiler-sfc": "3.0.0-rc.10",
        "css-loader": "^3.6.0",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-vue": "^7.0.0-beta.3",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.3.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.14.1",
        "prettier": "^2.0.5",
        "sass-loader": "^9.0.2",
        "style-loader": "^1.2.1",
        "terser-webpack-plugin": "^3.0.7",
        "ts-loader": "^8.0.2",
        "typescript": "^3.9.7",
        "url-loader": "^4.1.0",
        "vue-loader": "^16.0.0-beta.5",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "scripts": {
        "clean": "rm -rf dist/ target/",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "webpack",
        "build:prod": "NODE_ENV=production npm run build",
        "package": "mkdir -p target/ && tar -czf target/preserve-$npm_package_version.tar.gz --transform=s/dist/preserve-$npm_package_version/g dist/*",
        "lint:eslint": "eslint . --ext .js,.ts,.vue",
        "lint:prettier": "prettier --check .",
        "lint": "npm run lint:eslint && npm run lint:prettier",
        "fix:eslint": "eslint --fix .",
        "fix:prettier": "prettier --write .",
        "fix": "npm run fix:eslint && npm run fix:prettier",
        "start": "webpack-dev-server --hot",
        "serve": "PRESERVE_PATH=./dist node ./contrib/webserver-config/express/index.js"
    },
    "author": "Tony Finn <dev@tonyfinn.com>",
    "license": "GPL-2.0-or-later"
}
